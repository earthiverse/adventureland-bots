{
  "name": "adventureland-bots",
  "type": "module",
  "version": "2.0.0",
  "description": "Adventureland Bots based on ALClient",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npx copyfiles -u 1 config/**/*.json dist/config",
    "clean": "rm -r dist && rm ./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "format": "prettier --write .",
    "lint": "eslint",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=2 --coverage --collectCoverageFrom='{config,src}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthiverse/adventureland-bots.git"
  },
  "keywords": [
    "alclient",
    "adventureland"
  ],
  "author": "Kent Rasmussen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/earthiverse/adventureland-bots/issues"
  },
  "homepage": "https://github.com/earthiverse/adventureland-bots#readme",
  "dependencies": {
    "@isaacs/ttlcache": "^2.1.2",
    "alclient": "file:../../ALClient.worktrees/new-alclient/alclient-1.0.0.tgz",
    "config": "^4.1.0",
    "discord.js": "^14.19.3",
    "lru-cache": "^11.1.0",
    "nodemailer": "^7.0.5",
    "tinyqueue": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/config": "^3.3.5",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/nodemailer": "^7.0.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typed-adventureland": "^0.0.50",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
