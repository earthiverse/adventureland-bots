{
  "name": "adventureland-bots",
  "type": "module",
  "version": "2.0.0",
  "description": "Adventureland Bots based on ALClient",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npx copyfiles -u 1 config/**/*.json dist/config",
    "clean": "rm -r dist && rm ./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "lint": "eslint",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=2 --coverage --collectCoverageFrom='{config,src}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthiverse/adventureland-bots.git"
  },
  "keywords": [
    "alclient",
    "adventureland"
  ],
  "author": "Kent Rasmussen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/earthiverse/adventureland-bots/issues"
  },
  "homepage": "https://github.com/earthiverse/adventureland-bots#readme",
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.1",
    "@types/nodemailer": "^6.4.16",
    "alclient": "file:../ALClient/alclient-1.0.0.tgz",
    "config": "^3.3.12",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "lru-cache": "^11.1.0",
    "nodemailer": "^7.0.2",
    "tinyqueue": "^3.0.0",
    "typescript-eslint": "^8.32.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/config": "^3.3.5",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.15.16",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typed-adventureland": "^0.0.46",
    "typescript": "^5.8.3"
  }
}
